# WalletConnect Project ID (Required)
# Get this from https://cloud.walletconnect.com
VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# MongoDB Connection (Required)
MONGODB_URI=mongodb+srv://<db_username>:<db_password>@shadowwallet.ssdgqxr.mongodb.net/?retryWrites=true&w=majority&appName=ShadowWallet

# App Configuration
VITE_APP_NAME=ShadowWallet
VITE_APP_DESCRIPTION=Zero-knowledge privacy for your crypto transactions

# Network Configuration (Optional - defaults to mainnet + testnets)
VITE_ENABLE_TESTNETS=true

# Analytics (Optional)
VITE_ANALYTICS_ID=

# Environment
NODE_ENV=production
